{
	"name": "df_hechos_ventas",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dim_detalle_ventas",
						"type": "DatasetReference"
					},
					"name": "origen1"
				},
				{
					"name": "origen2"
				},
				{
					"dataset": {
						"referenceName": "ds_silver_productos",
						"type": "DatasetReference"
					},
					"name": "origen3"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          sls_ord_num as string,",
				"          sls_prd_key as string,",
				"          sls_cust_id as string,",
				"          sls_order_dt as string,",
				"          sls_ship_dt as string,",
				"          sls_sales as string,",
				"          sls_quantity as string,",
				"          sls_price as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> origen1",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> origen2",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> origen3",
				"origen1 derive(sls_order_dt = toDate(sls_order_dt, 'yyyy-MM-dd'),",
				"          sls_ship_dt = toDate(sls_ship_dt, 'yyyy-MM-dd'),",
				"          sls_sales = toDecimal(sls_sales),",
				"          sls_quantity = toInteger(sls_quantity),",
				"          sls_price = toDecimal(sls_price)) ~> derivedColumn1",
				"derivedColumn1, origen2 join(sls_cust_id == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}