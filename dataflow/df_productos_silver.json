{
	"name": "df_productos_silver",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dim_productos",
						"type": "DatasetReference"
					},
					"name": "Origen1"
				},
				{
					"dataset": {
						"referenceName": "dim_productos_CAT_MAN",
						"type": "DatasetReference"
					},
					"name": "origen2"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          prd_id as string,",
				"          prd_key as string,",
				"          prd_nm as string,",
				"          prd_cost as string,",
				"          prd_line as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Origen1",
				"source(output(",
				"          ID as string,",
				"          CAT as string,",
				"          SUBCAT as string,",
				"          MAINTENANCE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> origen2",
				"Origen1 derive(prd_key = substring(prd_key, 1,6)) ~> derivedColumn1",
				"derivedColumn1 derive(prd_key = replace(prd_key, '-', ' ')) ~> derivedColumn3",
				"derivedColumn3, origen2 join(prd_key == ID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}